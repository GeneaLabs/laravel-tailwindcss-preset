{
    "name": "genealabs/laravel-tailwindcss-preset",
    "type": "library",
    "description": "Laravel 5 frontend preset for Tailwind CSS",
    "keywords": [
        "laravel",
        "preset",
        "tailwindcss"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Bronner",
            "email": "hello@genealabs.com"
        }
    ],
    "require": {
        "illuminate/support": "5.5 - 5.6"
    },
    "require-dev": {
        "laravel/laravel": "5.6"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelTailwindcssPreset\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelTailwindcssPreset\\Service"
            ]
        }
    }
}
